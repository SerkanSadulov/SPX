  <data name="GetAllServices" xml:space="preserve">
    <value>SELECT 
    [ServiceID], 
    [ProviderID], 
    [ServiceName], 
    [CategoryID], 
    [Price], 
    [Description], 
    [ContactPhone], 
    [CreatedOn], 
    [Images]
FROM 
    [Services];</value>
  </data>
  <data name="GetMessagedPeople" xml:space="preserve">
    <value>SELECT DISTINCT
    CASE
        WHEN [SenderID] = @UserId THEN [ReceiverID]
        ELSE [SenderID]
    END AS ContactUserID,
    MAX([SentOn]) AS LastSentOn
FROM [Messages]
WHERE [SenderID] = @UserId OR [ReceiverID] = @UserId
GROUP BY 
    CASE
        WHEN [SenderID] = @UserId THEN [ReceiverID]
        ELSE [SenderID]
    END
ORDER BY LastSentOn DESC;</value>
  </data>
