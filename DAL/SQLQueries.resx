  <data name="SelectHighestRated" xml:space="preserve">
    <value>SELECT *
FROM [Ratings]
WHERE [ServiceID] IN (
    SELECT [ServiceID]
    FROM [Ratings]
    GROUP BY [ServiceID]
    HAVING AVG([Rating]) &gt; 4
);</value>
  </data>
  <data name="SelectMessages" xml:space="preserve">
    <value>SELECT * 
FROM [Messages] 
WHERE 
    ([SenderID] = @SenderID AND [ReceiverID] = @ReceiverID) 
    OR ([SenderID] = @ReceiverID AND [ReceiverID] = @SenderID)
ORDER BY [SentOn];</value>
  </data>
  <data name="SelectMessagesByRecieverID" xml:space="preserve">
    <value>SELECT TOP 3
       [MessageID],
       [SenderID],
       [ReceiverID],
       [MessageContent],
       [SentOn],
       [MessageStatus]
FROM  [Messages]
WHERE [ReceiverID] = @ReceiverID
ORDER BY [SentOn] DESC;</value>
  </data>
  <data name="SelectMessagesBySenderAndReciever" xml:space="preserve">
    <value>SELECT DISTINCT ReceiverID AS ContactUserID
FROM Messages
WHERE SenderID = @UserID

UNION

SELECT DISTINCT SenderID AS ContactUserID
FROM Messages
WHERE ReceiverID = @UserID</value>
  </data>
